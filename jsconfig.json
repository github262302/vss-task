{
    "compilerOptions": {
        "baseUrl": ".",
        "checkJs": true,
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "vss/*": [
                "./electron/*"
            ]
        },
        "target": "ESNext",
        "module": "CommonJS",
        "jsx": "react-jsx",
        "lib": ["dom","ESNext","ES2015"],
        "allowImportingTsExtensions": true
    },
    "include": [
        "./electron/**/*.js",
        "./src/**/*.js",
        "./src/**/*.jsx",
        "./src/**/*.tsx",
        "./src/**/*.ts",
    ]
}
